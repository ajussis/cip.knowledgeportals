/*---------- GENERAL STYLE ----------*/
body {
  background-color:#eee;
  }
#visual-portal-wrapper {
  background-color:#FFFFFF;
  margin-left:auto !important;
  margin-right:auto !important;
  position:static;
  width:963px;
  }
  
/*---------- PORTAL TOP ----------*/
#portal-top {
  /*margin:0 10px;*/
  margin:0;
  }
#portal-header {
  background:url(&dtml-portal_url;/header.gif) no-repeat 0 0 #581a74;
  height:166px;
  position:relative;
  z-index:3;
  }
.notify #portal-header {
  height:auto;
  }
  
/*---------- PORTAL SITEACTIONS ----------*/
#portal-siteactions {
  bottom:0;
  left:0;
  /*opacity:0.7;*/
  padding:6px 0 5px;
  position:relative;
  text-align:center;
  float:none;
  }
#portal-siteactions li {
  margin:0;
  border-right:1px dotted #DDDDDD;
  }
#portal-siteactions li a {
  border:0;
  color:#DDDDDD;
  font-family:Verdana, Geneva, sans-serif;
  margin:0;
  padding:0 9px;
  text-transform:none;
  }
#portal-siteactions li a:hover {
  border:0;
  color:#3a3a3a;
  }

/*---------- PORTAL LOGO ----------*/
#portal-logo {
  display:none;
  height:122px;
  margin-top:23px;
  overflow:hidden;
  width:259px;
  }
#portal-logo img {
  margin:<dtml-var "(122-_[logoName].height)/2">px 0 0 <dtml-var "(259-_[logoName].width)/2">px;
  }

/*---------- GLOBAL NAVIGATION ----------*/
#portal-globalnav {
background-color:#5C1B77;
background-image:url("action.jpg");
background-position:right center;
background-repeat:no-repeat;
border-top:medium none;
line-height:12px;
list-style-image:none;
list-style-type:none;
margin-top:146px;
overflow:hidden;
padding:0 0 0 1.2em;
}

#portal-globalnav li {
border-right:2px solid white;
float:left;
margin:3px -1px 3px 0;
}


#portal-globalnav li.lastItem {
  border-right:0;
  }
#portal-globalnav li a:hover, #portal-globalnav li.selected a {
background:none repeat scroll 0 0 transparent;
border:0 none;
color:#FFFFFF;
}
#portal-globalnav .selected a, #portal-globalnav a:hover {
background:transparent;
color:White;
}
#portal-globalnav li a {
border:0 none;
color:#FFFFFF;
display:block;
font-weight:bold;
margin:0;
padding:0 0;
text-decoration:none;
text-transform:none;
}
#portal-globalnav li a:hover, #portal-globalnav li.selected a {
background:none repeat scroll 0 0 transparent;
border:0 none;
color:#FFFFFF;
}
.themeComment {
  left:-9999px;
  position:absolute;
  }
  
